{
    "name": "yosymfony/spress-plugin-more-tag",
    "type": "spress-plugin",
    "description": "More tag for post in Spress sites",
    "keywords": ["spress", "plugin", "markdown", "excerpt"],
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Puertas",
            "email": "vpgugr@gmail.com",
            "homepage": "http://yosymfony.com"
        }
    ],
    "require": {
        "spress/spress-installer": "2.0.*"
    },
    "extra": {
        "spress_class": "SpressPlugins\\SpressMoreTag\\SpressMoreTag",
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "SpressPlugins\\SpressMoreTag\\": ""
        }
    }
}
